{
    "name": "ecotone/amqp",
    "license": [
        "Apache-2.0",
        "proprietary"
    ],
    "homepage": "https://docs.ecotone.tech/",
    "forum": "https://discord.gg/CctGMcrYnV",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "support@simplycodedsoftware.com"
        }
    ],
    "keywords": [
        "amqp",
        "rabbitmq",
        "rabbit",
        "messaging",
        "asynchronous",
        "message",
        "enterprise integration patterns",
        "eip",
        "distributed architecture",
        "domain driven design"
    ],
    "description": "Extends Ecotone with AMQP integration",
    "autoload": {
        "psr-4": {
            "Ecotone\\Amqp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\Amqp\\": [
                "tests"
            ]
        }
    },
    "require": {
        "ext-amqp": "*",
        "ecotone/enqueue": "~1.225.0",
        "enqueue/amqp-ext": "^0.10.18",
        "enqueue/dsn": "^0.10.4",
        "enqueue/enqueue": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "psr/container": "^2.0",
        "doctrine/annotations": "^1.13",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "scripts": {
        "tests:phpstan": "vendor/bin/phpstan",
        "tests:phpunit": [
            "vendor/bin/phpunit"
        ],
        "tests:ci": [
            "@tests:phpstan",
            "@tests:phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.225.0-dev"
        },
        "ecotone": {
            "repository": "amqp"
        },
        "merge-plugin": {
            "include": [
                "../local_packages.json"
            ]
        },
        "license-info": {
            "Apache-2.0": {
                "name": "Apache License 2.0",
                "url": "https://github.com/ecotoneframework/ecotone-dev/blob/main/LICENSE",
                "description": "Allows to use non Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            },
            "proprietary": {
                "name": "Enterprise License",
                "description": "Allows to use Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}