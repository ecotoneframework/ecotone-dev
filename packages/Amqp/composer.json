{
    "name": "ecotone/amqp",
    "license": [
        "Apache-2.0",
        "proprietary"
    ],
    "homepage": "https://docs.ecotone.tech/",
    "forum": "https://discord.gg/GwM2BSuXeg",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "support@simplycodedsoftware.com"
        }
    ],
    "keywords": [
        "amqp",
        "rabbitmq",
        "rabbit",
        "messaging",
        "asynchronous",
        "message",
        "enterprise integration patterns",
        "eip",
        "distributed architecture",
        "domain driven design"
    ],
    "description": "Extends Ecotone with AMQP integration",
    "autoload": {
        "psr-4": {
            "Ecotone\\Amqp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\Amqp\\": [
                "tests"
            ]
        }
    },
    "require": {
        "ext-amqp": "*",
        "ecotone/enqueue": "~1.294.0",
        "enqueue/amqp-ext": "^0.10.18",
        "enqueue/dsn": "^0.10.4",
        "enqueue/enqueue": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "phpstan/phpstan": "^1.8",
        "doctrine/annotations": "^1.13|^2.0",
        "wikimedia/composer-merge-plugin": "^2.1",
        "ext-amqp": "*",
        "enqueue/amqp-ext": "^0.10.18",
        "enqueue/amqp-lib": "^0.10.25"
    },
    "suggest": {
        "ext-amqp": "Required for using AMQP extension (default, recommended for production). Install together with enqueue/amqp-ext.",
        "enqueue/amqp-ext": "Required for using AMQP extension (default, recommended for production). Requires ext-amqp.",
        "enqueue/amqp-lib": "Required for using AMQP lib (pure PHP implementation). Required for AmqpStreamChannelBuilder and RabbitMQ Streams support."
    },
    "conflict": {
        "enqueue/amqp-ext": "<0.10.18",
        "enqueue/amqp-lib": "<0.10.25"
    },
    "scripts": {
        "tests:phpstan": "vendor/bin/phpstan",
        "tests:phpunit": [
            "vendor/bin/phpunit --no-coverage --testdox"
        ],
        "tests:phpunit:amqp-ext": [
            "AMQP_IMPLEMENTATION=ext vendor/bin/phpunit --no-coverage"
        ],
        "tests:phpunit:amqp-lib": [
            "AMQP_IMPLEMENTATION=lib vendor/bin/phpunit --no-coverage"
        ],
        "tests:ci": [
            "@tests:phpstan",
            "@tests:phpunit:amqp-ext",
            "@tests:phpunit:amqp-lib"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.294.0-dev"
        },
        "ecotone": {
            "repository": "amqp"
        },
        "merge-plugin": {
            "include": [
                "../local_packages.json"
            ]
        },
        "license-info": {
            "Apache-2.0": {
                "name": "Apache License 2.0",
                "url": "https://github.com/ecotoneframework/ecotone-dev/blob/main/LICENSE",
                "description": "Allows to use non Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            },
            "proprietary": {
                "name": "Enterprise License",
                "description": "Allows to use Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            }
        },
        "release-time": "2026-01-14 17:16:31"
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}