{
    "name": "ecotone/enqueue",
    "license": [
        "Apache-2.0",
        "proprietary"
    ],
    "homepage": "https://docs.ecotone.tech/",
    "forum": "https://discord.gg/CctGMcrYnV",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "support@simplycodedsoftware.com"
        }
    ],
    "keywords": [
        "enqueue",
        "ecotone"
    ],
    "description": "Integration with enqueue",
    "autoload": {
        "psr-4": {
            "Ecotone\\Enqueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\": [
                "tests"
            ]
        }
    },
    "require": {
        "ecotone/ecotone": "~1.224.0",
        "queue-interop/queue-interop": "^0.8",
        "enqueue/dsn": "^0.10.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.10",
        "phpstan/phpstan": "^1.8",
        "wikimedia/composer-merge-plugin": "^2.0",
        "enqueue/null": "^0.10.18"
    },
    "scripts": {
        "tests:phpstan": "vendor/bin/phpstan",
        "tests:phpunit": [
            "vendor/bin/phpunit"
        ],
        "tests:ci": [
            "@tests:phpstan",
            "@tests:phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.224.0-dev"
        },
        "ecotone": {
            "repository": "enqueue"
        },
        "merge-plugin": {
            "include": [
                "../local_packages.json"
            ]
        },
        "license-info": {
            "Apache-2.0": {
                "name": "Apache License 2.0",
                "url": "https://github.com/ecotoneframework/ecotone-dev/blob/main/LICENSE",
                "description": "Allows to use non Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            },
            "proprietary": {
                "name": "Enterprise License",
                "description": "Allows to use Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}