{
    "name": "ecotone/sqs",
    "license": [
        "Apache-2.0",
        "proprietary"
    ],
    "homepage": "https://docs.ecotone.tech/",
    "forum": "https://discord.gg/CctGMcrYnV",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "support@simplycodedsoftware.com"
        }
    ],
    "keywords": [
        "ecotone",
        "Sqs"
    ],
    "description": "Extends Ecotone with Amazon Sqs integration",
    "autoload": {
        "psr-4": {
            "Ecotone\\Sqs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\Sqs\\": [
                "tests"
            ]
        }
    },
    "require": {
        "ecotone/enqueue": "~1.225.0",
        "enqueue/sqs": "^0.10.15",
        "aws/aws-sdk-php": "<=3.269.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.10",
        "phpstan/phpstan": "^1.8",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "scripts": {
        "tests:phpstan": "vendor/bin/phpstan",
        "tests:phpunit": "vendor/bin/phpunit",
        "tests:behat": "vendor/bin/behat -vvv",
        "tests:ci": [
            "@tests:phpstan",
            "@tests:phpunit",
            "@tests:behat"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.225.0-dev"
        },
        "ecotone": {
            "repository": "sqs"
        },
        "merge-plugin": {
            "include": [
                "../local_packages.json"
            ]
        },
        "license-info": {
            "Apache-2.0": {
                "name": "Apache License 2.0",
                "url": "https://github.com/ecotoneframework/ecotone-dev/blob/main/LICENSE",
                "description": "Allows to use non Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            },
            "proprietary": {
                "name": "Enterprise License",
                "description": "Allows to use Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}