{
    "name": "ecotone/tempest",
    "license": [
        "Apache-2.0",
        "proprietary"
    ],
    "homepage": "https://docs.ecotone.tech/",
    "forum": "https://discord.gg/CctGMcrYnV",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "support@simplycodedsoftware.com"
        }
    ],
    "keywords": ["ecotone", "tempest"],
    "description": "Extends Ecotone with Tempest integration",
    "autoload": {
        "psr-4": {
            "Ecotone\\Tempest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ecotone\\Tempest\\": [
                "tests"
            ]
        }
    },
    "require": {
        "ecotone/ecotone": "^1.62",
        "tempest/core": "^1.0",
        "tempest/container": "^1.0",
        "tempest/discovery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.5|^11.0",
        "phpstan/phpstan": "^1.8",
        "psr/container": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.1",
        "tempest/framework": "^1.0"
    },
    "scripts": {
        "tests:phpstan": "vendor/bin/phpstan",
        "tests:phpunit": "vendor/bin/phpunit",
        "tests:ci": [
            "@tests:phpstan",
            "@tests:phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.62-dev"
        },
        "ecotone": {
            "repository": "tempest"
        },
        "merge-plugin": {
            "include": [
                "../local_packages.json"
            ]
        },
        "license-info": {
            "Apache-2.0": {
                "name": "Apache License 2.0",
                "url": "https://github.com/ecotoneframework/ecotone-dev/blob/main/LICENSE",
                "description": "Allows to use non Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            },
            "proprietary": {
                "name": "Enterprise License",
                "description": "Allows to use Enterprise features of Ecotone. For more information please write to support@simplycodedsoftware.com"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}
