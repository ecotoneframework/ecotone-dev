{
    "type": "lendable-playground",
    "license": "MIT",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ecotone\\": "../../packages/Ecotone/src",
            "Ecotone\\EventSourcing\\": "../../packages/PdoEventSourcing/src",
            "Ecotone\\JMSConverter\\": "../../packages/JmsConverter/src",
            "Ecotone\\Lite\\": "../../packages/LiteApplication/src"
        }
    },
    "require": {
        "ecotone/lite-event-sourcing-starter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "../../config.json"
            ]
        }
    }
}
