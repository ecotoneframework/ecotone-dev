{
    "name": "ecotone/quickstart-examples",
    "type": "application",
    "homepage": "https://docs.ecotone.tech/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": [
        "symfony",
        "ecotone",
        "ddd",
        "messaging",
        "message",
        "enterprise integration patterns",
        "eip",
        "distributed architecture"
    ],
    "description": "Extends Ecotone with Symfony integration",
    "scripts": {
        "tests:ci": [
            "(cd Asynchronous && composer update && php run_example.php)",
            "(cd Conversion && composer update && php run_example.php)",
            "(cd CQRS && composer update && php run_example.php)",
            "(cd EmittingEventsFromProjection && composer update && php run_example.php)",
            "(cd EventHandling && composer update && php run_example.php)",
            "(cd EventSourcing && composer update && php run_example.php)",
            "(cd WorkingWithAggregateDirectly && composer update && php run_example.php)",
            "(cd Microservices && composer update && php run_example.php)",
            "(cd MicroservicesAdvanced && composer update && php run_example.php)",
            "(cd Schedule && composer update && php run_example.php)",
            "(cd StatefulProjection && composer update && php run_example.php)",
            "(cd OutboxPattern && composer update && php run_example.php)",
            "(cd RefactorToReactiveSystem && composer update && php run_example.php Stage_1 && php run_example.php Stage_2 && php run_example.php Stage_3)",
            "(cd ErrorHandling && composer update && php run_example.php)",
            "(cd Testing && composer update && php run_example.php && vendor/bin/phpunit tests)",
            "(cd BuildingBlocks && composer update && php run_example.php && vendor/bin/phpunit tests)"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.204.0-dev"
        },
        "ecotone": {
            "repository": "quickstart-examples"
        }
    }
}